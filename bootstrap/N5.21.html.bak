	 <script type="text/javascript" language="javascript">
	function showzb(id){
		for(var i=1;i<=5;i++){
			document.getElementById("d"+i).className="";
			document.getElementById("zb"+i).style.display="none";
		}
		document.getElementById("d"+id).className="zbbb";
		document.getElementById("zb"+id).style.display="block";
	}

	
  </script>
